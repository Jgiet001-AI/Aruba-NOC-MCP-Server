# Aruba Central API Configuration
# Copy this file to .env and fill in your values

# API Base URL (optional - defaults to US1)
ARUBA_BASE_URL=https://us1.api.central.arubanetworks.com

# OAuth2 Credentials (required if not using access token)
ARUBA_CLIENT_ID=your_client_id
ARUBA_CLIENT_SECRET=your_client_secret

# Pre-generated Access Token (alternative to OAuth2 credentials)
ARUBA_ACCESS_TOKEN=your_access_token
